    </LinearLayout>

    <LinearLayout
        android:id="@+id/ll_zjjs"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/white"
            app:layout_constraintTop_toTopOf="parent"
            android:textSize="@dimen/sp_14"
            app:layout_constraintStart_toStartOf="parent"
            android:text="职级晋升和调整消化："/>

        <TextView
            android:id="@+id/tv_zjjs"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:text="20"
            android:textSize="@dimen/sp_14"
            app:layout_constraintTop_toTopOf="@+id/tv_xhrs_title"
            android:textColor="@color/white"
            app:layout_constraintStart_toEndOf="@+id/tv_xhrs_title"/>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/ll_dltx"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/white"
            app:layout_constraintTop_toTopOf="parent"
            android:textSize="@dimen/sp_14"
            app:layout_constraintStart_toStartOf="parent"
            android:text="到龄退休："/>

        <TextView
            android:id="@+id/tv_dltx"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:text="20"
            android:textSize="@dimen/sp_14"
            app:layout_constraintTop_toTopOf="@+id/tv_xhrs_title"
            android:textColor="@color/white"
            app:layout_constraintStart_toEndOf="@+id/tv_xhrs_title"/>

    </LinearLayout>

</LinearLayout>